# number of even numbers
count = 0

# message requests for range of numbers
message1 = "Enter first number: "
message2 = "Enter last number: "


# takes user input for range of numbers
u1 = int(input(message1))
u2 = int(input(message2))


# print and count even numbers within range
for numbers in range(u1, u2):
    if(numbers % 2 == 0 and numbers != u2):
        print(numbers)
        count += 1

print(f"We have {count} even numbers")
